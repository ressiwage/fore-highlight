{
    "name": "fore-syntax-highlighting",
    "displayName": "fore syntax highlighting",
    "description": "Provides syntax highlighting in fore language",
    "version": "0.1.10",
    "publisher": "AnsonYeung",
    "license": "MIT",
    "icon": "images/Pascal.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/ressiwage/fore-highlight"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pascal",
                "aliases": [
                    "Pascal",
                    "pascal"
                ],
                "extensions": [
                    ".pas",
                    ".pp"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pascal",
                "scopeName": "source.pascal",
                "path": "./syntaxes/pascal.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "prepare": "js-yaml syntaxes/pascal.tmLanguage.yaml > syntaxes/pascal.tmLanguage.json",
        "clean": "rm -f syntaxes/pascal.tmLanguage.json",
        "patch": "npm run prepare && vsce publish patch && npm run clean",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@vscode/test-cli": "^0.0.9",
        "@vscode/test-electron": "^2.3.10",
        "js-yaml": "^3.13.1",
        "vsce": "^2.7.0"
    }
}
